<div id="main-content">
	<div id="main-content-middle">
		<div class="section span-24 last">
			<div id="tagline" class="span-8 append-1">
				<h2>Browse. Accept. Invest.</h2>
			</div>
			<%= render :partial => "fund_status" %>
		</div>
		<div class="span-24 last">
			<div class="invites-heading prepend-1 span-23 last">
				Deals by <strong>Equeety Users</strong>
			</div>
			<div class="prepend-1 span-23 last">
				<div class="invite span-5">
					<div class="invite-title">
						<h2>Equeety</h2>
					</div>
					<div class="invite-status">New</div>
					
					<div class="invite-info">
						<div class="deal-field">
							<div class="short-label">ROUND</div>
							<div class="short-value">Pre-Seed</div>
						</div>
						<div class="deal-field">
							<div class="short-label">COMMITMENTS</div>
							<div class="short-value">5</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>